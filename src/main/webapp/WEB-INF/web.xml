<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Project2</display-name>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>csci310.servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>csci310.servlets.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddStock</servlet-name>
        <servlet-class>csci310.servlets.AddStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveStock</servlet-name>
        <servlet-class>csci310.servlets.RemoveStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadProfile</servlet-name>
        <servlet-class>csci310.servlets.LoadProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>csci310.servlets.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddStock</servlet-name>
        <url-pattern>/AddStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveStock</servlet-name>
        <url-pattern>/RemoveStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadProfile</servlet-name>
        <url-pattern>/LoadProfile</url-pattern>
    </servlet-mapping>
</web-app>